{
  "name": "gtfs-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server:dev": "export NODE_ENV=development && nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"export NODE_ENV=development && npm run server:dev\" \"cd public && npm run app:dev\"  "
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "mta-gtfs": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "gtfs-realtime-bindings": "0.0.4",
    "nodemon": "^1.17.5",
    "request": "^2.87.0"
  }
}
